<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.MyListFragment">



    <View
        android:id="@+id/viewDivider3"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/primaryDivider"
        app:layout_constraintBottom_toTopOf="@+id/logout"/>
        <TextView
            android:id="@+id/userId"
            android:layout_width="117dp"
            android:layout_height="45dp"
            android:layout_marginStart="44dp"
            android:paddingStart="10dp"
            android:text="@string/login_please"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintBaseline_toBaselineOf="@+id/logout"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="160dp"
            android:text="@string/logout"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.655"
            app:layout_constraintStart_toEndOf="@+id/bottom_navigation3"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.099" />
    <View
        android:id="@+id/viewDivider2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/primaryDivider"
        app:layout_constraintBottom_toBottomOf="@+id/logout"/>
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="448dp"
        android:background="@color/colorAccent"
        app:layout_constraintTop_toBottomOf="@+id/logout"
        app:menu="@menu/menu_bottom"
        tools:layout_editor_absoluteX="0dp" />

    <View
        android:id="@+id/viewDivider"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/primaryDivider"
        app:layout_constraintBottom_toTopOf="@+id/bottom_navigation3"/>

    <FrameLayout
        android:id="@+id/fragmentContainer"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/viewDivider"
        app:layout_constraintTop_toTopOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>